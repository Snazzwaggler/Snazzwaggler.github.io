{{page-title "Projects"}}


<ul>
{{#each @model as |project|}}
    <Project::Project 
        @name={{project.name}}
        @image={{project.display_image}}
        @description={{project.description}}
        @id={{project.id}}
    />
{{else}}
    Uh-oh! Where did all the projects go?
{{/each}}
</ul>
